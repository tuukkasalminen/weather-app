{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "react-app",
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-useless-catch": 0,
        "linebreak-style": 0,
        "import/no-named-as-default": 0,
        "import/no-named-as-default-member": 0,
        "react/jsx-filename-extension": 0,
        "arrow-parens": 0,
        "global-require": 0,
        "react/prop-types": 0,
        "jsx-a11y/no-static-element-interactions": 0,
        "jsx-a11y/click-events-have-key-events": 0,
        "no-return-await": 0,
        "prefer-template": 0,
        "consistent-return": 0
    }
}